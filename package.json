{
  "name": "backpack-ios",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^8.12.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "preinstall": "npx ensure-node-env",
    "test": "eslint . --ext .js,.jsx",
    "build": "npm run test && gulp",
    "prettier": "prettier --config .prettierrc --write \"**/*.js\"",
    "gulp": "gulp"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skyscanner/backpack-ios.git"
  },
  "author": "Backpack Design System <backpack@skyscanner.net>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Skyscanner/backpack-ios/issues"
  },
  "homepage": "https://github.com/Skyscanner/backpack-ios#readme",
  "devDependencies": {
    "bpk-svgs": "^6.0.2",
    "bpk-tokens": "^27.4.0",
    "del": "^3.0.0",
    "eslint-config-skyscanner-with-prettier": "^0.4.0",
    "eslint_d": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-data": "^1.3.1",
    "gulp-nunjucks": "^4.0.0",
    "gulp-rename": "^1.2.2",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.4",
    "lodash": "^4.17.11",
    "merge2": "^1.2.1",
    "through2": "^2.0.3",
    "tinycolor2": "^1.4.1"
  }
}
