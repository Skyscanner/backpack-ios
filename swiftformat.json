{
  "rules": {
    "blankLinesAroundMark": true,
    "blankLinesAtEndOfScope": true,
    "blankLinesAtStartOfScope": true,
    "blankLinesBetweenScopes": true,
    "braces": true,
    "consecutiveBlankLines": true,
    "consecutiveSpaces": true,
    "duplicateImports": true,
    "elseOnSameLine": true,
    "fileHeader": true,
    "hoistPatternLet": true,
    "indent": true,
    "linebreakAtEndOfFile": true,
    "linebreaks": true,
    "numberFormatting": true,
    "ranges": true,
    "redundantBackticks": true,
    "redundantGet": true,
    "redundantInit": true,
    "redundantLet": true,
    "redundantNilInit": true,
    "redundantParens": true,
    "redundantPattern": true,
    "redundantRawValues": true,
    "redundantReturn": true,
    "redundantSelf": true,
    "redundantVoidReturnType": true,
    "semicolons": true,
    "sortedImports": true,
    "spaceAroundBraces": true,
    "spaceAroundBrackets": true,
    "spaceAroundComments": true,
    "spaceAroundGenerics": true,
    "spaceAroundOperators": true,
    "spaceAroundParens": true,
    "spaceInsideBraces": true,
    "spaceInsideBrackets": true,
    "spaceInsideComments": true,
    "spaceInsideGenerics": true,
    "spaceInsideParens": true,
    "specifiers": true,
    "strongOutlets": true,
    "todos": true,
    "trailingClosures": false,
    "trailingCommas": true,
    "trailingSpace": true,
    "unusedArguments": true,
    "void": true,
    "wrapArguments": true
  },
  "options": {
      "--allman": "false",
      "--wraparguments": "disabled",
      "--wrapelements": "disabled",
      "--self": "remove",
      "--header": "ignore",
      "--binarygrouping": "none",
      "--octalgrouping": "none",
      "--indentcase": "false",
      "--trimwhitespace": "always",
      "--decimalgrouping": "none",
      "--patternlet": "hoist",
      "--commas": "always",
      "--semicolons": "inline",
      "--indent": "4",
      "--exponentcase": "lowercase",
      "--operatorfunc": "spaced",
      "--elseposition": "same-line",
      "--empty": "void",
      "--ranges": "spaced",
      "--hexliteralcase": "uppercase",
      "--linebreaks": "lf",
      "--hexgrouping": "none",
      "--comments": "indent",
      "--ifdef": "indent",
      "--stripunusedargs": "closure-only"
  }
}
